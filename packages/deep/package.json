{
  "name": "@rimbu/deep",
  "version": "0.5.8",
  "description": "Tools to use handle plain JS objects as immutable objects",
  "keywords": [
    "immutable",
    "patch",
    "match",
    "plain object",
    "freeze",
    "frozen",
    "rimbu",
    "typescript"
  ],
  "homepage": "http://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/deep"
  },
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/types/index.d.ts",
  "umd:main": "dist/umd/index.js",
  "unpkg": "dist/umd/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "bundle": "yarn bundle:main && yarn bundle:module && yarn bundle:types && yarn bundle:umd",
    "bundle:main": "tsc --p tsconfig.main.json",
    "bundle:module": "tsc --p tsconfig.module.json",
    "bundle:types": "tsc --p tsconfig.types.json",
    "bundle:umd": "microbundle -f umd --tsconfig tsconfig.umd.json -o dist/umd/index.js",
    "clean": "rimraf dist",
    "format": "yarn format:base --write",
    "format:base": "prettier \"{!CHANGELOG.md}|**/*.{ts,tsx,js,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "test": "jest",
    "test:types": "tsd",
    "typecheck": "tsc"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@rimbu/base": "^0.5.8",
    "@rimbu/common": "^0.5.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
