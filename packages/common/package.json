{
  "name": "@rimbu/common",
  "version": "0.5.2",
  "description": "Common types and objects used in many other Rimbu packages",
  "keywords": [
    "common",
    "range",
    "reducer",
    "immutable",
    "rimbu",
    "typescript"
  ],
  "homepage": "http://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/common"
  },
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "bundle": "yarn bundle:main && yarn bundle:module && yarn bundle:types",
    "bundle:main": "tsc --p tsconfig.main.json",
    "bundle:module": "tsc --p tsconfig.module.json",
    "bundle:types": "tsc --p tsconfig.types.json",
    "clean": "rimraf dist",
    "format": "yarn format:base --write",
    "format:base": "prettier \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "yarn format:base --check",
    "lint": "eslint src",
    "prepublish": "yarn build",
    "test": "jest",
    "typecheck": "tsc"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
