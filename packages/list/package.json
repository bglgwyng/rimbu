{
  "name": "@rimbu/list",
  "version": "0.5.1",
  "description": "An efficient immutable ordered sequence of elements akin to a Vector",
  "keywords": [
    "list",
    "vector",
    "random access",
    "sequence",
    "elements",
    "trie",
    "immutable",
    "collection",
    "rimbu",
    "typescript"
  ],
  "homepage": "http://rimbu.org",
  "author": {
    "name": "Arvid Nicolaas",
    "email": "arvid@rimbu.org",
    "url": "https://github.com/vitoke"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/vitoke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimbu-org/rimbu.git",
    "directory": "packages/list"
  },
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn bundle",
    "bundle": "yarn bundle:main && yarn bundle:module && yarn bundle:types",
    "bundle:main": "tsc --p tsconfig.main.json",
    "bundle:module": "tsc --p tsconfig.module.json",
    "bundle:types": "tsc --p tsconfig.types.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "test": "jest",
    "test:random": "jest --config jest-random.config.json",
    "test:types": "tsd",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*": [
      "yarn format:single",
      "eslint"
    ]
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@rimbu/base": "^0.5.1",
    "@rimbu/collection-types": "^0.5.1",
    "@rimbu/common": "^0.5.1",
    "@rimbu/stream": "^0.5.1"
  }
}
